{
  "functions": {
    "api/*.php": { "runtime": "vercel-php@0.7.1" }
  },
  "routes": [
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/sw.js", "dest": "/sw.js" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    
    { "src": "/login_ui.php", "dest": "/api/login_ui.php" },
    { "src": "/register_ui.php", "dest": "/api/register_ui.php" },
    { "src": "/tracker_ui.php", "dest": "/api/tracker_ui.php" },
    { "src": "/chat_ui.php", "dest": "/api/chat_ui.php" },
    { "src": "/profile_ui.php", "dest": "/api/profile_ui.php" },
    { "src": "/forgot_password_ui.php", "dest": "/api/forgot_password_ui.php" },
    { "src": "/logout.php", "dest": "/api/logout.php" },
    
    { "src": "/login.php", "dest": "/api/login.php" },
    { "src": "/register.php", "dest": "/api/register.php" },
    { "src": "/profile.php", "dest": "/api/profile.php" },
    { "src": "/chat.php", "dest": "/api/chat.php" },
    { "src": "/tracker.php", "dest": "/api/tracker.php" },
    { "src": "/export_logs.php", "dest": "/api/export_logs.php" },
    { "src": "/generate_meal.php", "dest": "/api/generate_meal.php" },

    { "src": "/dashboard", "dest": "/api/tracker_ui.php" },
    { "src": "/tracker", "dest": "/api/tracker_ui.php" },
    { "src": "/tracker.php", "dest": "/api/tracker_ui.php" },
    { "src": "/chat", "dest": "/api/chat_ui.php" },
    { "src": "/chat.php", "dest": "/api/chat_ui.php" },
    { "src": "/meal_prep", "dest": "/api/meal_prep.php" },
    { "src": "/meal_prep.php", "dest": "/api/meal_prep.php" },
    { "src": "/login", "dest": "/api/login_ui.php" },
    { "src": "/register", "dest": "/api/register_ui.php" },
    { "src": "/", "dest": "/api/index.php" },
    { "src": "/index.php", "dest": "/api/index.php" }
  ]
}